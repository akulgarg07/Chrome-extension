{
"manifest_version": 2,
"name": "Quick Search",
"short_name": "Easy Serach",
"version": "1.0",
"description": "This is your Easily QUICK SEARCH and Excess Media Extension",
"author" : "Purushottam Sharma",
"content_scripts": [
    {
    "matches": [
        "*://*/*"
        ],
    "js": ["fn.js"],
    "run_at" : "document_end",
    "all_frames" : true
    }
],
"icons":{
	"128": "icon128.png",
	"48": "icon48.png",
	"16": "icon16.png"
},
"browser_action": {
		"default_icon": "icon16.png",
		"default_popup": "popup.html"
}
}

 



